<link rel="stylesheet" href="/css/user_signin.css">
<form action="/user/createSession" method="POST">
    <input class="form-control" type="email" name="email" placeholder="Enter Email..." required>
    <input class="form-control" type="password" name="password" placeholder="Enter Password..." required>
    <input class="btn btn-success" type="submit" value="Sign In">
</form>

<form action="/user/auth/google">
    <button class="btn btn-primary">Google SignIn</button>
</form>

<form action="/user/emailVerify">
    <button class="btn btn-dark">Reset Password</button>
</form>